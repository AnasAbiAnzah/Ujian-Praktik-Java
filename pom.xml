<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	  http://www.liquibase.org/xml/ns/dbchangelog-2.0.xsd">
	  
	  <changeSet id="1" author="anas">
	  <createTable tableName="tabel-liquibase">
	  <column name="id" type="int">
	  <constraints primaryKey="true" nullable="false"/>
	  </column>
	  <column name="nama" type="varchar(50)">
	  <constraints nullable="false"/>
	  </column>
	  <column name="kelas" type="varchar(20)">
	  <constraints nullable="true"/>
	  </column>
	  <column name="jurusan" type="varchar(45)">
	  <constraints nullable="true"/>
	  </column>
	  <column name="jenis_kelamin" type="enum('p','l')">
	  <constraints nullable="true"/>
	  </column>
	  <column name="alamat" type="text">
	  <constraints nullable="true"/>
	  </column>
	  
	  </createTable>
	  </changeSet>
	  
	  </databaseChangeLog>